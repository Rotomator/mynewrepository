SELECT * FROM EMP


CREATE TABLE #PRO
(
PID INT,
PNAME VARCHAR(100)
)

CREATE TABLE #ORDER
(
OID INT,
PID INT,
NAME VARCHAR(100),
QUNATITY  INT
)
INSERT INTO #ORDER
VALUES(3,3,'DDDD',10)


SELECT P.PID, P.PNAME,O.PID, O.NAME, O.QUNATITY
FROM #PRO P INNER JOIN #ORDER O
	ON P.PID = O.PID 

SELECT P.PID, P.PNAME,O.PID, O.NAME, O.QUNATITY
FROM #PRO P FULL OUTER JOIN #ORDER O
	ON P.PID = O.PID 

CREATE TABLE ##PRO
(
PID INT,
PNAME VARCHAR(100)
)


