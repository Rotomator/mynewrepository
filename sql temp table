SELECT * FROM #PRO,#ORDER


SELECT * FROM #PRO P ,#ORDER O
WHERE P.PID = O.PID 

SELECT A.ID, .... 
	FROM #ORDER A  INNER JOIN #ORDER B
			 ON A.CITY = B.CITY 
				 AND A.COUNTRY = B.COUNTRY 
				 

SELECT *
FROM #ORDER O 
WHERE CITY = (SELECT TOP 1 CITY FROM #ORDER WHERE CITY = O.CITY) 
  AND COUNTRY= (SELECT TOP 1 COUNTRY FROM #ORDER WHERE COUNTRY = O.COUNTRY) 


SELECT C.CUSTIID , C.CUSTNAME, COUNT(O.QUNATITY)
FROM CUSTOMER C INNER JOIN ORDERS O 
	ON C.CUSTIID = O.CUSTID 
GROUP BY C.CUSTIID , C.CUSTNAME

